# fixed effects
park +
# global smooths
s(month, bs = "cc", k = 4) + #month effect
s(year, k = 8) + #year effect
ti(month, year, k = 6), #month/ year interaction
family = "betar",
#beta location scale distribution for the data
data = NDVI_2000_2021,
method = 'fREML'
)
